{
    "version": "2",
    "sql": [
      {
        "schema": "store/migrations",
        "queries": "store/queries",
        "engine": "postgresql",
        "gen": {
          "go": {
            "package": "store",
            "out": "store",
            "sql_package": "pgx/v4",
            "emit_empty_slices": true,
            "emit_db_tags": true,
            "overrides": [
              {
                "go_type": "string",
                "db_type": "citext"
              }
            ]
          }
        }
      }
    ]
  }
  